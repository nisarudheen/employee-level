<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="employee_level_action" model="ir.actions.act_window">
        <field name="name">EmployeeLevel</field>
        <field name="res_model">employee.level</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="employee_level_view_tree" model="ir.ui.view">
        <field name="name">employee.level.tree</field>
        <field name="model">employee.level</field>
        <field name="arch" type="xml">
            <tree>
                <field name="level"/>
                <field name="salary"/>
            </tree>
        </field>
    </record>
     <record id="employee_level_view_form" model="ir.ui.view">
        <field name="name">employee.level.form</field>
        <field name="model">employee.level</field>
        <field name="arch" type="xml">
            <form>
                <seet>
                    <group>
                        <field name="level"/>
                        <field name="salary"/>
                    </group>
                </seet>
            </form>
        </field>
    </record>
</odoo>